{
  "name": "typescrpt-nodejes-setup",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon ./build/index.js",
    "build": "babel src --extensions \".ts,.js\" --out-dir build --copy-files",
